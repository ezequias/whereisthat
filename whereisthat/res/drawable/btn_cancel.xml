<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#f67809" />
            <stroke
                android:width="1dp"
                android:color="#dd6c09" />
            <corners
                android:radius="3dp" />                
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#faa85e"
                android:endColor="#ff7c09"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ff7c09" />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
</selector>